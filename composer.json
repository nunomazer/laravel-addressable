{
    "name": "nunomazer/laravel-addressable",
    "description": "Laravel Addressable is a polymorphic Laravel package, for addressbook management. You can add addresses to any eloquent model with ease.",
    "type": "library",
    "keywords": [
        "city",
        "state",
        "model",
        "postal",
        "street",
        "address",
        "laravel",
        "country",
        "primary",
        "billing",
        "shipping",
        "eloquent",
        "latitude",
        "longitude",
        "addressable",
        "polymorphic"
    ],
    "license": "MIT",
    "homepage": "https://mazer.dev",
    "support": {
        "email": "ademir.mazer.jr@gmail.com",
        "issues": "https://github.com/nunomazer/laravel-addressable/issues",
        "source": "https://github.com/nunomazer/laravel-addressable",
        "docs": "https://github.com/nunomazer/laravel-addressable/blob/master/README.md"
    },
    "authors": [
        {
            "name": "Rinvex LLC",
            "homepage": "https://rinvex.com",
            "email": "help@rinvex.com"
        },
        {
            "name": "Abdelrahman Omran",
            "homepage": "https://omranic.com",
            "email": "me@omranic.com",
            "role": "Author of the based package"
        },
        {
            "name": "Ademir Mazer Junior",
            "homepage": "https://mazer.dev",
            "email": "ademir.mazer.jr@gmail",
            "role": "Project Lead"
        },
        {
            "name": "The Generous Laravel Community",
            "homepage": "https://github.com/nunomazer/laravel-addressable/contributors"
        }
    ],
    "require": {
        "php": "^7.4.0 || ^8.0.0",
        "illuminate/console": "^8.0.0 || ^9.0.0",
        "illuminate/database": "^8.0.0 || ^9.0.0",
        "illuminate/support": "^8.0.0 || ^9.0.0",
        "jackpopp/geodistance": "^1.2.0",
        "rinvex/countries": "^7.0.0",
        "rinvex/laravel-support": "^5.0.0"
    },
    "require-dev": {
        "codedungeon/phpunit-result-printer": "^0.30.0",
        "illuminate/container": "^8.0.0 || ^9.0.0",
        "phpunit/phpunit": "^9.5.0"
    },
    "autoload": {
        "psr-4": {
            "NunoMazer\\Addressable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NunoMazer\\Addressable\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "NunoMazer\\Addressable\\Providers\\AddressableServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
